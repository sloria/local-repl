{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "eqeqeq": 0,
        "arrow-body-style": 0,
        "import/no-dynamic-require": 0,
        "no-console": 0,
        "no-multi-assign": 0,
        "comma-dangle": 0,
        "prefer-destructuring": 0
    }
}
